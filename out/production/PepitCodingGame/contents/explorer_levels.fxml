<AnchorPane prefHeight="555.0" prefWidth="903.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.games.toolbox.explorer.ExplorerLevelController">
    <children>
        <Label layoutX="364.0" layoutY="40.0" text="Explorer" textAlignment="CENTER" underline="true">
            <effect>
                <Reflection />
            </effect>
            <font>
                <Font name="Century Gothic" size="24.0" />
            </font>
        </Label>
        <Button layoutX="531.0" layoutY="56.0" mnemonicParsing="false" onAction="#onCreateLevel" prefHeight="64.0" prefWidth="164.0" style="-fx-background-color: peru; -fx-background-radius: 40;" text="+ Create Level">
            <effect>
                <DropShadow />
            </effect>
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <TableView fx:id="tab_level_explorer" layoutX="11.0" layoutY="157.0" prefHeight="384.0" prefWidth="881.0">
            <columns>
                <TableColumn fx:id="column_explorer_question" prefWidth="199.0" text="Question" />
                <TableColumn fx:id="column_explorer_answer1" minWidth="0.0" prefWidth="180.0" text="Reponse1" />
                <TableColumn fx:id="column_explorer_answer2" prefWidth="134.0" text="Reponse2" />
                <TableColumn fx:id="column_explorer_answer3" prefWidth="151.0" text="Reponse3" />
                <TableColumn fx:id="column_explorer_correctAnswer" prefWidth="151.0" text="correctAnswer" />
            </columns>
            <effect>
                <InnerShadow />
            </effect>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </TableView>
        <Button fx:id="btn_delete_level_explorer"  layoutX="716.0" layoutY="56.0" mnemonicParsing="false"  prefHeight="64.0" prefWidth="164.0" style="-fx-background-color: peru; -fx-background-radius: 40;" text="- Delete Level">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <effect>
                <DropShadow />
            </effect>
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
